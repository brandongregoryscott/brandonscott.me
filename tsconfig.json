{
    "compilerOptions": {
        "module": "ESNext",
        "target": "ESNext",
        "moduleResolution": "Node",
        "strictNullChecks": true,
        "skipLibCheck": true,
        "jsx": "react-jsx",
        "jsxImportSource": "jsx-async-runtime"
    },
    "include": ["src/**/*.tsx", "src/**/*.ts"],
    "exclude": ["node_modules", "_site"]
}
